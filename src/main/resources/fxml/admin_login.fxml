      </TextField>
      <Button fx:id="loginBut" layoutX="287.0" layoutY="339.0" mnemonicParsing="false" onAction="#handleButtonOnAction" prefHeight="37.0" prefWidth="96.0" style="-fx-background-color: #DC4C4C;" text="LOGIN">
         <font>
            <Font name="Tw Cen MT" size="20.0" />
         </font>
      </Button>
      <Button fx:id="backBut" layoutX="24.0" layoutY="18.0" mnemonicParsing="false" onAction="#handleButtonOnAction" prefHeight="37.0" prefWidth="77.0" style="-fx-background-color: lightgreen;" text="BACK">
         <font>
            <Font name="Tw Cen MT" size="20.0" />
         </font>
      </Button>
      <ImageView fitHeight="140.0" fitWidth="163.0" layoutX="265.0" layoutY="67.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../pic/admin.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
