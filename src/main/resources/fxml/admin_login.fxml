<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" style="-fx-background-color: lightblue;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="condo.controller.AdminLoginController">
   <children>
      <Label layoutX="265.0" layoutY="14.0" text="ADMIN">
         <font>
            <Font name="Tw Cen MT" size="40.0" />
         </font>
      </Label>
      <Label layoutX="92.0" layoutY="226.0" text="USERNAME">
         <font>
            <Font name="Tw Cen MT" size="25.0" />
         </font>
      </Label>
      <Label layoutX="87.0" layoutY="282.0" text="PASSWORD">
         <font>
            <Font name="Tw Cen MT" size="25.0" />
         </font>
      </Label>
      <PasswordField fx:id="adminPassword" alignment="CENTER" layoutX="220.0" layoutY="278.0" prefHeight="25.0" prefWidth="249.0" promptText="ENTER PASSWORD">
         <font>
            <Font name="Tw Cen MT" size="20.0" />
         </font>
      </PasswordField>
      <TextField fx:id="adminUser" alignment="CENTER" layoutX="220.0" layoutY="223.0" prefHeight="25.0" prefWidth="249.0" promptText="ENTER USERNAME">
